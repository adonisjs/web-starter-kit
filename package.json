    "@adonisjs/tsconfig": "^1.1.7",
    "@japa/assert": "^2.0.0-1",
    "@japa/plugin-adonisjs": "^2.0.0-0",
    "@japa/runner": "^3.0.0-3",
    "@swc/core": "^1.3.67",
    "@types/node": "^20.3.3",
    "eslint": "^8.44.0",
